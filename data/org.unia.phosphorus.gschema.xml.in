<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="@GETTEXT_PACKAGE@">
  <schema path="/org/unia/phosphorus/" id="org.unia.phosphorus">
    <key name="directories" type="as">
      <default>['']</default>
      <summary>Directories to scan for wallpapers</summary>
      <description>An array of directories which Phosphorus will scan for wallpapers.</description>
    </key>
    <key name="recursion" type="b">
      <default>true</default>
      <summary>Recurse into directories when scanning for wallpapers</summary>
      <description>When scanning for wallpapers, recursion can be used to scan nested directories.</description>
    </key>
    <key name="active-plugins" type="as">
      <default>[@ACTIVE_PLUGINS@]</default>
      <summary>Active plugins</summary>
      <description>List of active plugins.</description>
    </key>
    <key name="wallpaper" type="s">
      <default>""</default>
      <summary>The previously applied wallpaper</summary>
      <description>The wallpaper previously applied by Phosphorus.</description>
    </key>
  </schema>
</schemalist>
